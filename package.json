{
  "name": "tablink.js",
  "version": "1.0.0",
  "keywords": [
    "tab",
    "scroll",
    "tabScroll",
    "link"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Embrace-oyo/tablink"
  },
  "bugs": {
    "url": "https://github.com/Embrace-oyo/tablink/issues"
  },
  "description": "Tabs linked to long content scrolling",
  "main": "src/main.js",
  "scripts": {
    "serve": "webpack server --config ./webpackConfig/webpack.dev.config.js",
    "build": "webpack --config ./webpackConfig/webpack.prod.config.js",
    "lint": "eslint src",
    "fix": "npm run lint --fix",
    "commit": "git-cz",
    "x": "npm --no-git-tag-version version major",
    "y": "npm --no-git-tag-version version minor",
    "z": "npm --no-git-tag-version version patch",
    "pub": "npm publish --access=public",
    "pub:x": "npm run x && npm publish --access=public",
    "pub:y": "npm run y && npm publish --access=public",
    "pub:z": "npm run z && npm publish --access=public"
  },
  "author": "kongjianqiu",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.18.2",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "node-notifier": "^10.0.1",
    "portfinder": "^1.0.28",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0"
  }
}
